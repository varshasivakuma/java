public class MissingNumbersSeries {
    public static void main(String[] args) {
        int[] series = {1, 5, 11, 19};
        int missingNumber = 0;
        for (int i = 0; i < series.length - 1; i++) {
            if (series[i + 1] - series[i] != 4) {
                missingNumber = series[i] + 4;
                System.out.println("Missing number in the series: " + missingNumber);
            }
        }
    }
}
